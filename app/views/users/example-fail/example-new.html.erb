<h1>Nuevo Usuario</h1>
<!-- con esta vista se creara un usurio -->
<% form_for(@user) do |form| %>
	<div class="field">
		<% form.label :nombre %><br>
		<% form.text_field :email %>
	</div>
	<div class="field">
		<% form.label :email %><br>
		<% form.text_field :email %>
	</div>
	<div class="actions">
		<%= form.submit 'Guardar' %>
	</div>
<% end %>
